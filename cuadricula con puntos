void setup (){
  size(500,500);
}


void draw (){
  background(255);
  
  
  for(int a = 10; a <400; a = a+ 40){
    for(int b = 10; b < 400; b = b+ 20){
      fill(160,70,40);
      ellipse (a,b,10,10);
    }
  }
  
  for (int c = 0; c < 400; c = c+10){
   
    line (c, 10,c, 400);
  }
  
    for (int d = 100; d < 400; d = d+10){
     
    line (10, d,400, d);
    }
}
